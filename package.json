        "@fortawesome/pro-duotone-svg-icons": ">5.0.0",
        "@fortawesome/pro-light-svg-icons": ">5.0.0",
        "@fortawesome/pro-regular-svg-icons": ">5.0.0",
        "@fortawesome/pro-solid-svg-icons": ">5.0.0",
        "@fortawesome/react-fontawesome": "x"
    },
    "devDependencies": {
        "@babel/core": "7.9.0",
        "@storybook/addon-actions": "^6.0.21",
        "@storybook/addon-essentials": "^6.0.21",
        "@storybook/addon-links": "^6.0.21",
        "@storybook/react": "^6.0.21",
        "@svgr/webpack": "4.3.3",
        "@testing-library/jest-dom": "^4.2.4",
        "@testing-library/react": "^9.3.2",
        "@testing-library/user-event": "^7.1.2",
